{
  "compilationOptions": {
    "followSymlinks": true,
    "preferredConfigPath": "./tsconfig.json"
  },
  "entries": [
    {
      "failOnClass": false,
      "filePath": "./src/index.ts",
      "libraries": {
        "allowedTypesLibraries": [],
        "importedLibraries": [],
        "inlinedLibraries": []
      },
      "noCheck": true,
      "outFile": "./dist/types/index.d.ts",
      "output": {
        "exportReferencedTypes": false,
        "inlineDeclareExternals": true,
        "inlineDeclareGlobals": false,
        "noBanner": true,
        "respectPreserveConstEnum": true,
        "sortNodes": true,
        "umdModuleName": "AlveLive"
      }
    }
  ]
}
