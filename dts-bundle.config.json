{
  "compilationOptions": {
    "followSymlinks": true,
    "preferredConfigPath": "./tsconfig.json"
  },
  "entries": [
    {
      "failOnClass": false,
      "filePath": "./src/index.ts",
      "libraries": {
        "allowedTypesLibraries": [
          "@alvelive/helpers",
          "@types/js-yaml",
          "@types/lodash-es",
          "@typescript-eslint/eslint-plugin",
          "cross-env",
          "eslint",
          "eslint-config-next",
          "eslint-config-prettier",
          "eslint-config-standard-with-typescript",
          "eslint-plugin-import",
          "eslint-plugin-local-rules",
          "eslint-plugin-n",
          "eslint-plugin-prettier",
          "eslint-plugin-promise",
          "eslint_d",
          "husky",
          "lint-staged",
          "lodash-es",
          "npm-run-all",
          "prettier",
          "prettier-plugin-packagejson",
          "typescript",
          "js-yaml"
        ],
        "importedLibraries": [
          "events"
        ],
        "inlinedLibraries": [
          "@alvelive/helpers",
          "@types/js-yaml",
          "@types/lodash-es",
          "@typescript-eslint/eslint-plugin",
          "cross-env",
          "eslint",
          "eslint-config-next",
          "eslint-config-prettier",
          "eslint-config-standard-with-typescript",
          "eslint-plugin-import",
          "eslint-plugin-local-rules",
          "eslint-plugin-n",
          "eslint-plugin-prettier",
          "eslint-plugin-promise",
          "eslint_d",
          "husky",
          "lint-staged",
          "lodash-es",
          "npm-run-all",
          "prettier",
          "prettier-plugin-packagejson",
          "typescript",
          "js-yaml"
        ]
      },
      "noCheck": true,
      "outFile": "./dist/types/index.d.ts",
      "output": {
        "exportReferencedTypes": false,
        "inlineDeclareExternals": true,
        "inlineDeclareGlobals": false,
        "noBanner": true,
        "respectPreserveConstEnum": true,
        "sortNodes": true,
        "umdModuleName": "AlveLive"
      }
    }
  ]
}